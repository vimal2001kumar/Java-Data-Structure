class Main{
public static void main(String[] args){
    int[] arr={-1,2,5,5,8,};
    int n=arr.length;
    int min=Integer.MAX_VALUE;
    int max=Integer.MIN_VALUE;
    for(int i=0;i<n;i++){
       if(arr[i]<min){
          min=arr[i];
       }
       else if(arr[i]>max){
            max=arr[i];
       }
    }
    System.out.println("min = "+ min+"  "+"max = "+max);
 }
}
